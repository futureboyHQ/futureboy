{
  "requests": {
    "^/$": "root",
    "^/team$": "team",
    "^/vision$": "vision",
    "^/weeklypurpose$": "weeklypurpose",
    "^/blog/(\\?.+=.+(&(.+=.+))*)?$": "blog",
    "^/blog/([a-zA-Z0-9\\-]{10,})$": "article",
    "404": "404"
  },
  "partials": {
    "404": {
      "path": "partials/404.html",
      "assets": {
        "styles": ["/assets/css/404.css"],
        "scripts": {}
      },
      "partials": ["head"]
    },
    "500": {
      "path": "static/500.html",
      "assets": {
        "styles": [],
        "scripts": {}
      },
      "partials": []
    },
    "index": {
      "path": "partials/index.html",
      "assets": {
        "styles": ["/assets/css/main.css"],
        "scripts": {"foot": ["{{bundle}}"]}
      },
      "partials": ["head", "foot", "navbar"]
    },
    "team": {
      "path": "partials/theCrew.html",
      "assets": {
        "styles": ["/assets/css/theCrewNIGHT.css"],
        "scripts": {"foot": ["{{bundle}}"]}
      },
      "partials": ["head", "footer_marketing", "navbar"]
    },
    "vision": {
      "type": "body",
      "path": "partials/wayOfLife.html",
      "assets": {
        "styles": ["/assets/css/wayOfLife.css"],
        "scripts": {"foot": ["{{bundle}}"]}
      },
      "partials": ["head", "footer_marketing", "navbar"]
    },
    "weeklypurpose": {
      "path": "partials/subscribe.html",
      "stylesheet": "/assets/css/subscribe.css",
      "partials": ["head", "foot", "navbar"]
    },
    "blog": {
      "type": "body",
      "path": "partials/feed.html",
      "assets": {
        "styles": ["/assets/css/blogNIGHT.css"],
        "scripts": {"foot": ["{{bundle}}"]}
      },
      "partials": ["head", "foot", "navbar"]
    },
    "article": {
      "type": "body",
      "path": "partials/article.html",
      "assets": {
        "styles": ["/assets/css/contentNIGHT.css"],
        "scripts": {"foot": ["{{bundle}}"]}
      },
      "partials": ["head", "navbar", "article_content", "foot"]
    },
    "head": {
      "type": "head",
      "path": "partials/header.html",
      "javascript": [],
      "assets": {
        "styles": ["/assets/css/normalize.css"],
        "scripts": {"head": ["https://use.typekit.net/rer0env.js"]}
      },
      "partials": []
    },
    "foot": {
      "type": "foot",
      "path": "partials/footer.html",
      "assets": {
        "styles": [],
        "scripts": {}
      },
      "partials": []
    },
    "footer_marketing": {
      "type": "foot",
      "path": "partials/footerMarketing.html",
      "assets": {
        "styles": [],
        "scripts": {}
      },
      "partials": []
    },
    "navbar": {
      "type": "body",
      "path": "partials/navbar.html",
      "assets": {
        "styles": [],
        "scripts": {}
      },
      "partials": []
    },
    "article_content": {
      "type": "body",
      "path": "articles/{{hash}}/index.html",
      "assets": {
        "styles": [],
        "scripts": {}
      },
      "partials": []
    },
    "feed_article": {
      "type": "body",
      "path": "partials/feed_article.html",
      "assets": {
        "styles": [],
        "scripts": {}
      },
      "partials": []
    }
  }
}
