{
  "requests": {
    "^/$": "root",
    "^/team$": "team",
    "^/vision$": "vision",
    "^/blog/(\\?.+=.+(&(.+=.+))*)?$": "blog",
    "^/blog/([a-zA-Z0-9\\-]{10,})$": "article",
    "^/subscribed": "subscribed",
    "^/error": "error",
    "404": "404"
  },
  "partials": {
    "404": {
      "path": "partials/404.html",
      "stylesheet": "/assets/css/404.css",
      "partials": ["head"]
    },
    "error": {
      "path": "partials/500.html",
      "stylesheet": "/assets/css/404.css",
      "partials": ["head"]
    },
    "index": {
      "path": "partials/index.html",
      "stylesheet": "/assets/css/main.css",
      "partials": ["head", "foot", "navbar"]
    },
    "team": {
      "path": "partials/theCrew.html",
      "stylesheet": "/assets/css/theCrewNIGHT.css",
      "partials": ["head", "footer_marketing", "navbar"]
    },
    "vision": {
      "path": "partials/wayOfLife.html",
      "stylesheet": "/assets/css/wayOfLife.css",
      "partials": ["head", "footer_marketing", "navbar"]
    },
    "subscribed": {
      "path": "partials/joinedNewsletter.html",
      "stylesheet": "/assets/css/joinedNewsletterNIGHT.css",
      "partials": ["head"]
    },
    "error": {
      "path": "partials/500.html",
      "stylesheet": "/assets/css/joinedNewsletterNIGHT.css",
      "partials": ["head"]
    },
    "blog": {
      "path": "partials/feed.html",
      "stylesheet": "/assets/css/blogNIGHT.css",
      "partials": ["head", "foot", "navbar"]
    },
    "article": {
      "path": "partials/article.html",
      "stylesheet": "/assets/css/contentNIGHT.css",
      "partials": ["head", "article_content", "foot", "navbar"]
    },
    "head": {
      "path": "partials/header.html",
      "stylesheet": null,
      "partials": []
    },
    "foot": {
      "path": "partials/footer.html",
      "stylesheet": null,
      "partials": []
    },
    "footer_marketing": {
      "path": "partials/footerMarketing.html",
      "stylesheet": null,
      "partials": []
    },
    "navbar": {
      "path": "partials/navbar.html",
      "stylesheet": null,
      "partials": []
    },
    "article_content": {
      "path": "articles/{{hash}}/index.html",
      "stylesheet": null,
      "partials": []
    },
    "feed_article": {
      "path": "partials/feed_article.html",
      "stylesheet": null,
      "partials": []
    }
  }
}
